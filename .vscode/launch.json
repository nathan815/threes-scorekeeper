    "version": "0.2.0",
    "configurations": [
      {
        "type": "node",
        "request": "attach",
        "name": "Attach Debugger",
        "protocol": "inspector",
        "port": 4321,
        "restart": true,
        "cwd": "${workspaceRoot}"
      },
      {
        "type": "node",
        "request": "launch",
        "name": "Run",
        "protocol": "inspector",
        "cwd": "${workspaceRoot}",
        "runtimeExecutable": "npm",
        "runtimeArgs": ["run-script", "start"]
      }
    ]
}
